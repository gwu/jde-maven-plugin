(jde-project-file-version "1.0")
(jde-set-variables
 '(jde-sourcepath
   (append
    jde-sourcepath
    (quote
     ("/my/source"
      "/your/source"))))
 '(jde-global-classpath
   (append
    jde-global-classpath
    (quote
     ("/my/classes"
      "/my/jar"))))
 '(jde-help-docsets
   (append
    jde-help-docsets
    (quote
     ("/a/path"
      "http://a.url")))))
